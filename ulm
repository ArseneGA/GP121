class Artiste:
    '''
    création de la classe artiste
    '''
    def __init__(self, nom, prenom, number, date, chaise_position, prix_place):
        '''
        création des attributs du artiste
        '''
        self.nom = nom
        self.prenom = prenom
        self.number = number
        self.date = date
        self.chaise_position = chaise_position
        self.prix_place = prix_place
    

